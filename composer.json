{
    "name": "ddpro/admin",
    "description": "DDPro Administrator Panel",
    "homepage": "https://github.com/ddpro/admin",
    "keywords": ["administrator", "admin", "database", "laravel-administrator", "laravel", "adminlte"],
    "license": "MIT",
    "authors": [
        {
            "name": "Del",
            "email": "del@babel.com.au"
        }
    ],
    "support": {
        "issues": "https://github.com/ddpro/admin/issues"
    },
    "require": {
        "php": ">=5.4.0",
        "delatbabel/viewpages": "dev-master",
        "delatbabel/fluents": "~1.0",
        "delatbabel/site-config": "~1.0",
        "illuminate/support": "5.1.*",
        "illuminate/view": "5.1.*",
        "ckeditor/ckeditor": "4.*",
        "rcrowe/twigbridge": "^0.9.2"
    },
    "require-dev": {
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "DDPro\\Admin\\": "src/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "if [ -d \"public/js/ckeditor/\" ] \nthen \nrm -R public/js/ckeditor/ \nfi ",
            "if [ -d \"vendor/ckeditor/ckeditor/\" ] \nthen \nmv vendor/ckeditor/ckeditor/ public/js/  \nfi",
            "if [ -d \"vendor/ckeditor/\" ] \nthen \nrm -R vendor/ckeditor/ \nfi"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
